Package: dparser
Title: Port of 'Dparser' Package
Version: 1.3.1-0
Authors@R: c(person("Matthew", "Fidler", email = "matthew.fidler@gmail.com", role = c("aut", "cre")),
	   person("John", "Plevyak", role = c("aut", "cph"), email = "jplevyak@gmail.com"))
Imports:
    digest,
